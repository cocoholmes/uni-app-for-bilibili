.contentContainer {
  width: 750rpx;
  height: 100%;
}
.contentContainer .header {
  display: -webkit-box;
  display: -webkit-flex;
  display: flex;
  padding: 15rpx 0;
  width: 100%;
}
.contentContainer .header .search {
  margin: 0 auto;
  position: relative;
  width: 700rpx;
  height: 75rpx;
  background: #f4f4f4;
  border-radius: 60rpx;
}
.contentContainer .header .search .iconfont {
  position: absolute;
  top: 18rpx;
  left: 10rpx;
  font-size: 36rpx;
}
.contentContainer .header .search input {
  width: 700rpx;
  margin-left: 50rpx;
  height: 75rpx;
}
.contentContainer .header .search input .placeholder {
  font-size: 26rpx;
}
.contentContainer .title {
  width: 100%;
  height: 70rpx;
  display: -webkit-box;
  display: -webkit-flex;
  display: flex;
  -webkit-justify-content: space-around;
          justify-content: space-around;
}
.contentContainer .title .title-pop {
  text-align: center;
  width: 70rpx;
  height: 50rpx;
}
.contentContainer .title .title-cartoon {
  width: 70rpx;
  text-align: center;
  height: 50rpx;
}
.contentContainer .title .active {
  border-bottom: 5rpx solid #fc7299;
  color: #fc7299;
}

